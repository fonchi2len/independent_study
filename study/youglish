<script>
function triggerLangPanel(close){
	if (close){
		$('#lang_sel').hide();
	}
	else {
		$('#lang_sel').slideToggle(100);
	}
}

function onLanguageChange(bIgnore, path){
		if (bIgnore) return;
	  window.location.href = path;
}
</script>
